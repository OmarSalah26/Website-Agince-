@model IEnumerable<RoleProject.View_Model.Report_Agince>
@using RoleProject.Models

@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_UserProfile.cshtml";
    ViewBag.Photo = ViewBag.Agince;
    ViewBag.id = ViewBag.AginceID;
}




    <div class="col-9" style="position:absolute;right:0px;">
        <h2 class="text-center" style="margin:20px;">Report  Cars</h2>
        <table class="table table-hover">

            <thead>
                <tr>
                    <th> </th>
                    <th>Date Recive</th>
                    <th>Date checkout</th>
                    <th>Date checkout</th>
                    <th>Client</th>
                </tr>
            </thead>




            @foreach (var item in Model)
            {
                for (int Start = 0, end = 0, price = 0, client = 0; (Start < item.Start_Recive.Count()) || (end < item.End_Recive.Count()) || (price < item.Total_Price.Count()) || (client < item.Clients.Count()); Start++, end++, price++, client++)
                {
                    <tr>
                        <td>
                            <a href="/Cars/Details/@item.Car.Car_Id"><img src="~/images/@item.Car.photo_Car" width="150" height="150" style="border-radius:50%" /></a>

                        </td>
                        @if (Start < item.Start_Recive.Count())
                        {
                            <td>
                                @item.Start_Recive[Start]
                            </td>
                        }
                        @if (end < item.End_Recive.Count())
                        {
                            <td>
                                @item.End_Recive[end]
                            </td>
                        }
                        @if (price < item.Total_Price.Count())

                        {
                            <td>
                                @item.Total_Price[price]
                            </td>
                        }
                        @if (client < item.Clients.Count())
                        {
                            <td>
                                @item.Clients[client].Name
                            </td>
                        }

                    </tr>

                }
            }


        </table>

    </div>