
 
    @using (Html.BeginForm("Go_Search_By_Chassia", "Cars", FormMethod.Get@*, htmlAttributes: new { style = "  position: relative;   left: 519px; " }*@))
    {
        <section class="ftco-section ftco-no-pb ftco-no-pt">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="search-wrap-1 ftco-animate mb-5">
                            <form action="#" class="search-property-1">
                                <div class="row">
                                    <div class="col-lg align-self-end">
                                        <div class="form-group">
                                            <div class="form-field">
                                                <input type="submit" value="Search" class="form-control btn btn-primary">
                                            </div>
                                        </div>
                                    </div>


                                    <div class="col-lg align-items-end">
                                        <div class="form-group">

                                            <div class="form-field">
                                                <div class="select-wrap">

                                                    @Html.TextBox("model", null, htmlAttributes: new { @class = "form-control", placeholder = " Model Of Car " })

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg align-items-end">
                                        <div class="form-group">

                                            <div class="form-field">
                                                <div class="select-wrap">
                                                    @Html.Editor("type", new { htmlAttributes = new { @class = "form-control", placeholder = " Type Of Car " } })



                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg align-items-end">
                                        <div class="form-group">

                                            <div class="form-field">
                                                <div class="select-wrap">

                                                    @Html.Editor("brand", new { htmlAttributes = new { @class = "form-control", placeholder = " Brand Of Car" } })


                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-lg align-items-end">
                                        <div class="form-group">

                                            <div class="form-field">
                                                <div class="select-wrap">

                                                    @Html.Editor("Chassis", new { htmlAttributes = new { @class = "form-control", placeholder = " Chassia number " } })


                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }

