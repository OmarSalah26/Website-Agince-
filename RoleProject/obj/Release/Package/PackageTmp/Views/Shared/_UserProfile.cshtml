@{ 
    var photo = ViewBag.Photo;
    var Name = ViewBag.Name;
    var id = ViewBag.id;
}
<link rel="stylesheet" href="~/Content/User profile/css/animate.css">

<link rel="stylesheet" href="~/Content/User profile/css/owl.carousel.min.css">
<link rel="stylesheet" href="~/Content/User profile/css/owl.theme.default.min.css">
<link rel="stylesheet" href="~/Content/User profile/css/magnific-popup.css">

<link rel="stylesheet" href="~/Content/User profile/css/aos.css">

<link rel="stylesheet" href="~/Content/User profile/css/ionicons.min.css">

<link rel="stylesheet" href="~/Content/User profile/css/bootstrap-datepicker.css">

<link rel="stylesheet" href="~/Content/User profile/css/jquery.timepicker.css">


<link rel="stylesheet" href="~/Content/User profile/css/flaticon.css">
<link rel="stylesheet" href="~/Content/User profile/css/icomoon.css">
<link rel="stylesheet" href="~/Content/User profile/css/style.css">
<aside id="colorlib-aside" role="complementary" class="js-fullheight text-center" style ="background-color:#fc983c">
    <h1 id="colorlib-logo"><span class="img" style="background-image: url('../../images/@photo');"></span>@Name</h1>
    <nav id="colorlib-main-menu" role="navigation">
        <ul>
            <li class="colorlib-active">@Html.ActionLink("Home", "Index", "HomeFolder")</li>
            @if (User.IsInRole("Admin"))
            {



                <li>@Html.ActionLink("Register Admin", "register", "Account")</li>
                <li>@Html.ActionLink("Users", "UsersWithRoles", "Account")</li>
                <li>@Html.ActionLink("Roles", "List_Of_All", "Roles")</li>
                <li> @Html.ActionLink(" Create New properties", "Create", "Car_Properties")</li>





            }
            @if (User.IsInRole("Agince"))
            {
                <li> @Html.ActionLink(" Create New properties", "Create", "Car_Properties")</li>

                <li>
                    @Html.ActionLink("Create New Car", "Create", "Cars")
                </li>
                <li>
                    @Html.ActionLink("Report Cars", "report", "Aginces")
                </li>
            }

            <li>@Html.ActionLink("Cars", "List_Of_All", "Cars")</li>
            <li>@Html.ActionLink("Aginces", "List_Of_All", "Aginces")</li>

            <li>@Html.ActionLink("Edit", "Edit", new { id = id })</li>
        </ul>
    </nav>


</aside>
@RenderBody()

<script src="~/Content/User profile/js/jquery.min.js"></script>
